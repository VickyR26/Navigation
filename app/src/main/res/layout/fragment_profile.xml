<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">


    <TextView
        android:id="@+id/textViewUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usuario"
        android:textSize="25dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />

    <EditText
        android:id="@+id/editTextUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="17"

        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@id/textViewUsuario"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />
    <TextView
        android:id="@+id/textViewNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/editTextUsuario"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp"/>

    <EditText
        android:id="@+id/editTextNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="17"
        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@id/textViewNombre"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />
    <TextView
        android:id="@+id/textViewApellidoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Paterno"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/editTextNombre"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />

    <EditText
        android:id="@+id/editTextApellidoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="17"
        android:inputType="textPersonName"

        app:layout_constraintTop_toBottomOf="@id/textViewApellidoP"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />

    <TextView
        android:id="@+id/textViewApellidoM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Materno"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/editTextApellidoP"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp"/>

    <EditText
        android:id="@+id/editTextApellidoM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="17"
        android:inputType="textPersonName"

        app:layout_constraintTop_toBottomOf="@id/textViewApellidoM"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp"/>
    <TextView
        android:id="@+id/textViewCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Correo"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/editTextApellidoM"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />

    <EditText
        android:id="@+id/editTextCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="17"
        android:inputType="textPersonName"

        app:layout_constraintTop_toBottomOf="@id/textViewCorreo"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp" />

    <Button
        android:id="@+id/buttonEnviar"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:text="ENVIAR"
        app:layout_constraintTop_toBottomOf="@id/editTextCorreo"
        android:layout_marginTop="35dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>